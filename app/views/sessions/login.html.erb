

<h1>Welcome to SurfBuddy</h1>
  <h1 class="subheader">Log In</h1>  

    <%= form_tag sessions_path do %>
   
      <%= label_tag :email  %><br />
      <%= text_field_tag :email, params[:email], title: "Enter email here", data: {tooltip: true} %>

      <%= label_tag :password %><br />
      <%= password_field_tag :password %>
    
      <%= submit_tag "Log In" %>
    <% end %>

     
